   <text>
      <body>
         <lg type="sonnet" xml:id="s344n_1198">
            <head>Soneto turco</head>
            <lg n="1" type="cuarteto">
               <l met="--+--+---+-" rhyme="A">Para bárbaros blancos, <w type="rhyme">Ghemorúk</w>
               </l>
               <l met="--+--+---+-" rhyme="B">Para zancos ridículos, <w type="rhyme">Albék</w>
               </l>
               <l met="---+-+---+-" rhyme="B">Para maderas vírgenes <w type="rhyme">Ilék</w>
               </l>
               <l met="-+---+---+-" rhyme="A">Para ágiles refígeros, <w type="rhyme">Ipúk</w>.</l>
            </lg>
            <lg n="2" type="cuarteto">
               <l met="---+-+---+-" rhyme="A">Para marfiles diáfanos, <w type="rhyme">Murzúk</w>
               </l>
               <l met="+-+--+---+-" rhyme="B">Par higiénicos lúpidos, <w type="rhyme">Pysék</w>
               </l>
               <l met="---+-+---+-" rhyme="B">Para tebáicos báquicos, <w type="rhyme">Hupék</w>
               </l>
               <l met="--+--+---+-" rhyme="A">Para dátiles cándidos, <w type="rhyme">Mabrúk</w>.</l>
            </lg>
            <lg n="3" type="terceto">
               <l met="--+--+---+-" rhyme="C">Para ostrícolas regias, <w type="rhyme">Chesapík</w>
               </l>
               <l met="--+--+---+-" rhyme="D">Para búfalos gordos, <w type="rhyme">Omenák</w>
               </l>
               <l met="--+--+---+-" rhyme="C">Para bálago helado, <w type="rhyme">Upernavík</w>.</l>
            </lg>
            <lg n="4" type="terceto">
               <l met="--+--+---+-" rhyme="D">Para bichos mortíferos, <w type="rhyme">Sihák</w>
               </l>
               <l met="++--+---+-" rhyme="C">Par tétricas focas, <w type="rhyme">Reykjhavík</w>
               </l>
               <l met="--+--+----+-" rhyme="D">Para kábila cruel, <w type="rhyme">Kjalikasák</w>.</l>
            </lg>
         </lg>
      </body>
   </text>
